<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <Version>3.1.0</Version>
    <Authors>Kuchulem</Authors>
    <Copyright>2024 - Kuchulem</Copyright>
    <PackageIcon>kuchulem-logo-transparent-bg-128.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://github.com/Kuchulem/DotNet.Extensions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Kuchulem/DotNet.Extensions.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>extensions;string;enumerable</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\kuchulem-logo-transparent-bg-128.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
